---
import Head from "../components/Head.astro";
import Footer from "../components/Footer.astro";
import Scripts from '../components/Scripts.astro';

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="fr">
  <Head title={title} />
  <body>
    <slot />
    <Footer />
    <Scripts />
  </body>
</html>
